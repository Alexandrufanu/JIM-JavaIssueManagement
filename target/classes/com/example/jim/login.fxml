<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="620.0" prefWidth="307.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jim.LoginController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to Java Issue Manager" wrappingWidth="175.39878845214844" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="To continue, please log in!" wrappingWidth="150.93669509887695" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Username" wrappingWidth="65.7991943359375" />
   <TextField fx:id="usr_txtField" onAction="#login" onInputMethodTextChanged="#login" promptText="Insert your username here" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
   <PasswordField fx:id="usr_passField" onAction="#login" prefHeight="0.0" prefWidth="162.0" promptText="Insert your password here" />
   <Button mnemonicParsing="false" onAction="#login" prefHeight="26.0" prefWidth="151.0" text="Log in" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="New to JIM? Let's get started!" />
   <Button mnemonicParsing="false" onAction="#signup" prefHeight="26.0" prefWidth="126.0" text="Sign Up" />
    <Label fx:id="login_error" layoutX="147.0" layoutY="586.0" textFill="RED" />

    <Label fx:id="welcomeText" />
</VBox>
